<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="fooOne" class="org.tony.springframework.study.autowiringbean.Foo"/>
    <bean id="barOne" class="org.tony.springframework.study.autowiringbean.Bar"/>

    <bean id="targetByName" autowire="byName"
          class="org.tony.springframework.study.autowiringbean.Target" lazy-init="true"/>

    <bean id="targetByType" autowire="byType"
          class="org.tony.springframework.study.autowiringbean.Target" lazy-init="true"/>

    <bean id="targetConstructor" autowire="constructor"
          class="org.tony.springframework.study.autowiringbean.Target" lazy-init="true"/>

</beans>